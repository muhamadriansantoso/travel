import { NgWizardConfig } from '../utils/interfaces';
import { Observable } from 'rxjs';
import { THEME } from '../utils/enums';
export declare class NgWizardService {
    private config;
    resetWizard$: Observable<any>;
    showNextStep$: Observable<any>;
    showPreviousStep$: Observable<any>;
    setTheme$: Observable<THEME>;
    private resetWizard;
    private showNextStep;
    private showPreviousStep;
    private setTheme;
    private defaultConfig;
    constructor(config: NgWizardConfig);
    getDefaultConfig(): NgWizardConfig;
    reset(): void;
    next(): void;
    previous(): void;
    theme(theme: THEME): void;
}
