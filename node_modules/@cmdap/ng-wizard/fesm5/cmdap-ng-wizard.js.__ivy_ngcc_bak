    NgWizardStep.prototype.wsOnNext = /**
     * @return {?}
     */
    function () {
        return;
    };
    /**
     * @return {?}
     */
    NgWizardStep.prototype.wsOnPrevious = /**
     * @return {?}
     */
    function () {
        return;
    };
